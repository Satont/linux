[title]
command=xtitle -s
interval=persist

[satont.ru]
command=~/.config/i3blocks/server-monitor/satont.ru; [[ -z "${BLOCK_BUTTON}" ]] || urxvt -e ssh root@satont.ru
interval=60
markup=pango


[twitchbot.ga]
command=~/.config/i3blocks/server-monitor/twitchbot.ga; [[ -z "${BLOCK_BUTTON}" ]] || urxvt -e ssh root@twitchbot.ga
interval=60
markup=pango
